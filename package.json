{
  "name": "cannaease-backend-core",
  "version": "1.0.0",
  "description": "Core microservice for the CannaEase Platform handling users, orders, and e-commerce logic.",
  "main": "server.js",
   "scripts": {
    "prebuild": "rimraf dist",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "node dist/main.js",
    "start:dev": "nodemon src/main.ts",
    "start:prod": "node dist/main.js",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "db:migration:generate": "typeorm migration:generate -d src/data-source.ts src/migrations/Migration",
    "db:migration:run": "typeorm migration:run -d src/data-source.ts"
  },
